<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.VueRouteMapper">

    <resultMap id="BaseResultMap" type="com.example.domain.VueRoute">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="authName" column="authName" jdbcType="VARCHAR"/>
            <result property="path" column="path" jdbcType="VARCHAR"/>
            <result property="father" column="father" jdbcType="BIGINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,authName,path,
        father
    </sql>
</mapper>
